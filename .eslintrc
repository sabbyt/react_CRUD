{
  "rules": {
    "indent": [2, 2],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "no-console": 0,
    "no-path-concat": 0,
    "quote-props": 0,
    "max-len": 0,
    "curly": 0,
    "arrow-parens": 0,
    "default-case": 0,
    "no-unused-vars": 0,
    "jsx-quotes": 1
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true,
    "jquery": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
  },
  "extends": "eslint:recommended"
}
